{
  "name": "ims",
  "version": "1.0.0",
  "description": "Inventory Management System ",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "seedData": "export $(grep -v '^#' .env | xargs) && node db/seedData.js",
    "start": "export $(grep -v '^#' .env | xargs) && nodemon index.js",
    "test": "npm run seedData && jest --detectOpenHandles",
    "test-windows": "(for /F \"tokens=*\" %i in (.env) do set %i) && node db/seedData.js && jest --detectOpenHandles"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.3",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "sequelize": "^6.37.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.2",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4"
  }
}
